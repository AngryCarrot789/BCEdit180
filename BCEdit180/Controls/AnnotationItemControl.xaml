<UserControl x:Class="BCEdit180.Controls.AnnotationItemControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:BCEdit180.Controls"
             xmlns:viewmodels="clr-namespace:BCEdit180.ViewModels" 
             d:DataContext="{d:DesignInstance Type=viewmodels:AnnotationViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="80" d:DesignWidth="400">
    <Grid>
        <local:DoubleClickEditBox Text="{Binding TypeName, UpdateSourceTrigger=PropertyChanged}" FontSize="14" FontFamily="Consolas"/>
    </Grid>
</UserControl>
