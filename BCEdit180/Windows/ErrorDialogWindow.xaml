<local:WindowModal x:Class="BCEdit180.Windows.ErrorDialogWindow"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:BCEdit180.Windows"
             xmlns:window="clr-namespace:BCEdit180.Core.Window;assembly=BCEdit180.Core"
             d:DataContext="{d:DesignInstance Type=window:ErrorDialogViewModel}"
             mc:Ignorable="d"
             Title="Action" Height="450" Width="800"
             Style="{DynamicResource CustomWindowBaseStyle}" Background="{DynamicResource AREghZyBrush.Deep.3.Background.Static}">
    <TextBox IsReadOnly="True" Text="{Binding Description, UpdateSourceTrigger=PropertyChanged}" TextWrapping="Wrap" FontSize="14" Padding="10" Background="Transparent" BorderThickness="0"/>
</local:WindowModal>
